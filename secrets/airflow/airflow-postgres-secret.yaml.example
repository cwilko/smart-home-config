# Usage: 
# kubeseal -f airflow-postgres-secret.yaml -w airflow-postgres-sealed-secret.yaml

apiVersion: v1 
kind: Secret 
metadata: 
  name: airflow-postgres-secret 
  namespace: actions
type: Opaque
stringData:
  POSTGRES_DB: <DB NAME>
  POSTGRES_USER: <DB USER>
  POSTGRES_PASSWORD: <DB PW>
  POSTGRES_HOST: <HOST>
  POSTGRES_PORT: <PORT>